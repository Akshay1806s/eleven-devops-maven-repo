pipeline {
    // add your slave label name
    agent { label 'my_slave1'}
    tools{
        maven 'maven'
    }
    stages {
        stage ('Checkout SCM') {

            steps {
			    git branch: 'main', url: 'https://github.com/sushmitha2506/skydevops-maven-repo.git'
            }
        }

        stage ('Build') {

            steps {
               sh 'mvn clean package'
            }
        }
        
       
        
    }
}
